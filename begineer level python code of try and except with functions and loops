def greet():
    a = str(input('ENTER YOUR NAME: '))
    print("hello master",a)

greet()    

try:
    b = input('ENTER YOUR PASSWORD TO ACCESS: ')
    c = len(b)
    if c > 10:
        print('THE PASSWORD LENGTH IS LONG')
    elif c < 8:
        print('THE PASSWORD IS TOO SMALL')
    else:
        print('THE PASSWORD IS STRONG')
except IndexError:
    print('THE PASSWORD IS NOT SUITABLE')   
    
